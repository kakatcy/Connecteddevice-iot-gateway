[ubidots.cloud]
apiKey         = Not Set
userNameToken  = Not Set
authToken      = Not Set
protocol       = ssl
host           = things.ubidots.com
port           = 8883
useWebAccess   = True
baseUrl        = https://things.ubidots.com/api/v1.6/
certFile       = ubidots_cert.pem
configPath     = config

[smtp.cloud]
host           = Not Set
port           = 465
fromAddr       = Not Set
toAddr         = Not Set
toMediaAddr    = Not Set
toTxtAddr      = Not Set
authToken      = Not Set
enableAuth     = True
enableCrypt    = True

[mqtt.cloud]
userNameToken  = Not Set
authToken      = Not Set
host           = test.mosquitto.org
port           = 1883
securePort     = 1884
defaultQos     = 0
enableAuth     = False
enableCrypt    = False
keepAlive      = 60

[device]
enableEmulator = True
pollCycleSecs  = 60
nominalTemp    = 20
